{{application-content-header model=model.header}}

<!-- Main content -->
<section class="content">
  <div class="row">
    <div class="col-md-3">
      <div class="box box-pane">
        <div class="box-body">
          {{date-range-picker title="Date" fieldId="dateRange"}}
          {{multiple-select selectId="office" title="Office" placeholder="Select a Office" options=model.offices}}
          {{multiple-select selectId="team" title="Team" placeholder="Select Team" options=model.teams}}
          {{multiple-select selectId="client" title="Clients" placeholder="Select Client" options=model.clients}}
        </div>
        <!-- /.box-body -->
      </div>
    </div>


    <div class="col-md-9">
      <!-- Projects -->
      <div class="box box-info">
        <div class="box-body">
          {{project-list-large projects=model.projects}}
        </div>
        <!-- /.box-body -->
      </div>
      <!-- /.box -->
    </div>
    <!-- /.col -->
  </div>
  <!-- /.row -->
</section>